{
	"name": "visavi/rotorcms",
	"description": "RotorCMS",
	"keywords": ["cms", "rotor"],
	"homepage": "http://visavi.net",
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Alexander Grigorev",
			"email": "visavi.net@mail.ru",
			"homepage": "http://pizdec.ru"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"nesbot/carbon": "~1.14",
		"illuminate/http": "~5.3",
		"illuminate/database": "~5.3",
		"illuminate/pagination": "~5.3",
		"robmorgan/phinx": "~0.5",
		"vlucas/phpdotenv": "~1.1",
		"visavi/compressor": "~1.1",
		"philo/laravel-blade": "~3.0",
		"pclzip/pclzip": "dev-master",
		"gregwar/captcha": "dev-master",
		"olamedia/nokogiri": "dev-master",
		"cbschuld/browser.php": "dev-master",
		"altorouter/altorouter": "~1.1",
		"paragonie/random_compat": "~1.1",
		"swiftmailer/swiftmailer": "~5.4",
		"abeautifulsite/simpleimage": "~2.5",
		"mobiledetect/mobiledetectlib": "dev-master",
		"php-curl-class/php-curl-class": "dev-master"
	},
	"require-dev": {
		"filp/whoops": "^2.0",
		"pear/log": "dev-master",
		"phpunit/phpunit": "^5.2",
		"fzaninotto/faker": "^1.6"
	},
	"scripts": {
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\""
		]
	},
	"autoload": {
		"psr-4": {
			"": [
				"app/classes/",
				"app/controllers/",
				"app/models/"
			]
		},
		"files": [
			"app/routes.php",
			"app/helpers.php"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "5.1-dev"
		}
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
